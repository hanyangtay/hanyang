<% provide(:title, "Projects") %>
<div class="static" id="projects">
<h1>Projects</h1>


<a href="#top" class="projects-up"> <i class="material-icons">keyboard_arrow_up</i> </a>

<p class="projects-first" id="top">
All my projects are live. Play with them by clicking this

<button id="others_mock" class="main-header mdl-navigation__link 
  mdl-button mdl-js-button mdl-js-ripple-effect mdl-button--icon">
  <i class="material-icons" role="presentation" id="projects-menu-demo">more_vert</i>
</button> at the top <span id="projects-menu-demo-2">right</span>.

<script type="text/javascript">
  document.addEventListener("turbolinks:load", function() {
    if($(window).width() <= 1024) {
      $('#projects-menu-demo').html("menu");
      $('#projects-menu-demo-2').html("left");
    }
    
    $("#others_mock").bind("click", function(){
      if($(window).width() > 1024){
        $("#others").click();}
      else {
        $('.mdl-layout__drawer-button').click();}
    });
  
    $(window).resize(function(){
      if ($(window).width() >1024) {
        $('#projects-menu-demo').html("more_vert");
        $('#projects-menu-demo-2').html("right");
      } 
      
      else {
        $('#projects-menu-demo').html("menu");
        $('#projects-menu-demo-2').html("left");
      };
    });
  });
    
</script>
</p>

<br />


<script>
document.addEventListener("turbolinks:load", function() {
  $('a').click(function(event) {
    var href = $.attr(this, 'href');
    var x_coord = $(href).offset().top - 50;
    $('main').animate({
        scrollTop: x_coord
    }, 'medium', function () {
        window.location.hash = href;
    });
    return false;
    
});
});
</script>

