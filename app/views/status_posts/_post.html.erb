

    
    <ul class="mdl-list" id="all-status-posts">
      <% if @status_posts.any? %>
      
      <% @status_posts.each do |status_post| %>
      
        <%= render partial: 'status_posts/post_entry', object: status_post, as: 'status_post' %>
        
      <% end %>
      
      <% end %>
    </ul>
    
    <%= paginate @status_posts %>
     
